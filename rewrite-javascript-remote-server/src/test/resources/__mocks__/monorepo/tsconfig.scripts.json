{
  "compilerOptions": {
    "target": "es2016",
    "module": "Node16",
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./scripts",
    "experimentalDecorators": true,
    "moduleResolution": "node16"
  },
  "include": ["scripts/**/*"]
}
